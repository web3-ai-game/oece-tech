{
  "autoSaveInterval": 3000,
  "formatOnSave": true,
  "terminal": {
    "shellProfile": "zsh",
    "inheritEnv": true,
    "envFiles": [".env.doppler", ".env.local"]
  },
  "ai": {
    "provider": "auto",
    "model": "auto",
    "apiKeyEnv": "OPENROUTER_API_KEY"
  },
  "extensions": {
    "enabled": [
      "ms-vscode-remote.remote-ssh",
      "eamodio.gitlens"
    ]
  }
}
