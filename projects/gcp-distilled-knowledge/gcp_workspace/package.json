{
  "name": "sms-key-vector-jet",
  "version": "2.0.0",
  "description": "SMS-Key 向量噴射引擎 - MongoDB + Supabase + Firebase",
  "main": "server-enhanced.js",
  "scripts": {
    "start": "node server.js",
    "start:enhanced": "node server-enhanced.js",
    "dev": "nodemon server.js",
    "dev:web": "cd web/app && npm run dev",
    "build:web": "cd web/app && npm run build",
    "test": "node test-api.js",
    "docker:build": "docker build -f Dockerfile.production -t vector-jet-api .",
    "docker:run": "docker run -p 8080:8080 --env-file .env.local vector-jet-api",
    "deploy": "bash deploy-to-cloudrun.sh",
    "deploy:quick": "gcloud run deploy sms-key-vector-jet --source . --region asia-southeast1",
    "logs": "gcloud run logs read --service sms-key-vector-jet --region asia-southeast1",
    "setup:mongodb": "node scripts/setup-mongodb.js",
    "setup:supabase": "node scripts/setup-supabase.js",
    "import:notion": "node scripts/import-notion-to-vector.js",
    "mongodb:setup": "node scripts/setup-mongodb.js",
    "supabase:setup": "node scripts/setup-supabase.js",
    "firebase:init": "firebase init"
  },
  "dependencies": {
    "@notionhq/client": "^5.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "mongodb": "^7.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "DeepWeay Labs",
  "license": "MIT",
  "keywords": [
    "vector-database",
    "gemini-api",
    "mongodb",
    "supabase",
    "cloud-run"
  ]
}
